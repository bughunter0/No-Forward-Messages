{
    "name": "No-Forward-Messages",
    "description": "A telegram bot for deleting forward messages.",
    "repository": "https://github.com/bughunter0/No-Forward-Messages",
    "keywords": [
        "telegram bot",
        "pyrogram"
    ],
    "env": {
        "SESSION_NAME": {
            "description": "Your Bot name or username"
        },
        "BOT_TOKEN": {
            "description": "Your Bot token from @Botfather"
        },
        "BOT_USERNAME": {
            "description": "Your Bot user name (with out @)"
        },
        "API_ID": {
            "description": "Your API_ID from https://my.telegram.org/apps"
        },
        "API_HASH": {
            "description": "Your API_HASH from https://my.telegram.org/apps"
        }
    },
    "buildpacks": [
        {
            "url": "heroku/python"
        }
    ]
}
